{
    "name": "resxpress",
    "displayName": "ResXpress",
    "license": "MIT",
    "description": "Preview and sort Resx files",
    "publisher": "PrateekMahendrakar",
    "icon": "images/logo.png",
    "repository": {
        "url": "https://github.com/pmahend1/resxpress.git"
    },
    "version": "3.0.0",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:resxpress.resxpreview",
        "onCommand:resxpress.sortbykeys",
        "onCommand:resxpress.newpreview"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "resxpress.resxpreview",
                "title": "ResXpress: Preview Resx"
            },
            {
                "command": "resxpress.sortbykeys",
                "title": "ResXpress: Sort By Keys"
            },
            {
                "command": "resxpress.newpreview",
                "title": "ResXpress: New Preview"
            }
        ],
        "configuration": {
            "title": "ResXpress"
        },
        "languages": [
            {
                "id": "xml",
                "extensions": [
                    ".resx"
                ]
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.1",
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.43.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.1",
        "ts-loader": "^8.0.2",
        "tslint": "^6.1.3",
        "typescript": "^3.9.7",
        "vscode-test": "^1.4.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "resx-parser": "^1.0.1",
        "xml-writer": "^1.7.0"
    }
}
