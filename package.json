{
    "name": "resxpress",
    "displayName": "ResXpress",
    "license": "MIT",
    "description": "Preview and sort Resx files",
    "publisher": "PrateekMahendrakar",
    "icon": "images/logo.png",
    "repository": {
        "url": "https://github.com/pmahend1/resxpress.git"
    },
    "version": "3.0.0",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:resxpress.resxpreview",
        "onCommand:resxpress.sortbykeys",
        "onCommand:resxpress.newpreview",
        "onCustomEditor:resx.editor",
        "onLanguage:resx"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "resx.editor",
                "displayName": "ResXpress Editor",
                "selector": [
                    {
                        "filenamePattern": "*.resx"
                    }
                ]
            }
        ],
        "commands": [
            {
                "command": "resxpress.resxpreview",
                "title": "ResXpress: Markdown Preview"
            },
            {
                "command": "resxpress.sortbykeys",
                "title": "ResXpress: Sort By Keys"
            },
            {
                "command": "resxpress.newpreview",
                "title": "ResXpress: Web Preview"
            }
        ],
        "configuration": {
            "title": "ResXpress"
        },
        "languages": [
            {
                "id": "resx",
                "extensions": [
                    ".resx"
                ]
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.1",
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.49.0",
        "@types/xml-js": "^1.0.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.1",
        "ts-loader": "^8.0.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3",
        "vscode-test": "^1.4.0",
        "webpack": "^5.1.0",
        "webpack-cli": "^4.0.0",
        "xml-js": "^1.6.11"
    },
    "dependencies": {}
}
